{
    "name": "amondar-libs/class-attributes",
    "description": "Allow postman documentation generation for any Laravel project.",
    "type": "library",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Sereda Oleg",
            "email": "zaxodu11@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "spatie/laravel-package-tools": "^1.16",
        "haydenpierce/class-finder": "^0.5.3",
        "illuminate/support": "^10|^11|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^4.1",
        "laravel/pint": "^1.24",
        "spatie/laravel-ray": "^1.41"
    },
    "autoload": {
        "psr-4": {
            "Amondar\\ClassAttributes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amondar\\ClassAttributes\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "ppest": "pest --parallel"
    }
}
